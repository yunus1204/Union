

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	int a[]={1,2,3,45};
	int b[]={5};
        Set<Integer> union =new HashSet<>();
        for(int i:a){
            union.add(i);
        }
        for(int i:b){
            union.add(i);
        }
       // return union.size();
       System.out.println(union);
       
    }
}
	
